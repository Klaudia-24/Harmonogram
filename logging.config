[loggers] %comment
keys=root,main,settingsWLogger,generalSetLogger,eventTypeSetLogger

[handlers]
keys=consoleHandler,fileHandler01, fileHandler02

[formatters]
keys=simpleFormatter

[logger_root]
level=DEBUG
handlers=consoleHandler

[logger_main]
level=DEBUG
handlers=consoleHandler,fileHandler01
qualname=main
propagate=0

[logger_settingsWLogger]
level=DEBUG
handlers=consoleHandler,fileHandler02
qualname=settingsWLogger
propagate=0

[logger_generalSetLogger]
level=DEBUG
handlers=consoleHandler,fileHandler02
qualname=generalSetLogger
propagate=0

[logger_eventTypeSetLogger]
level=DEBUG
handlers=consoleHandler,fileHandler02
qualname=eventTypeSetLogger
propagate=0

[handler_consoleHandler]
class=StreamHandler
level=DEBUG
formatter=simpleFormatter
args=(sys.stdout,)

[handler_fileHandler01]
class=FileHandler
level=DEBUG
formatter=simpleFormatter
args=('main.log','a')

[handler_fileHandler02]
class=FileHandler
level=DEBUG
formatter=simpleFormatter
args=('settings.log','a')

[formatter_simpleFormatter]
class=logging.Formatter
format=%(asctime)s - %(name)-12s - %(levelname)-8s - %(message)s