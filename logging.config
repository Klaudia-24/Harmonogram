[loggers] %comment
keys=root,main

[handlers]
keys=consoleHandler,fileHandler01

[formatters]
keys=simpleFormatter

[logger_root]
level=DEBUG
handlers=consoleHandler

[logger_main]
level=DEBUG
handlers=consoleHandler,fileHandler01
qualname=main
propagate=0

[handler_consoleHandler]
class=StreamHandler
level=DEBUG
formatter=simpleFormatter
args=(sys.stdout,)

[handler_fileHandler01]
class=FileHandler
level=DEBUG
formatter=simpleFormatter
args=('main.log','a')

[formatter_simpleFormatter]
class=logging.Formatter
format=%(asctime)s: %(name)s - %(levelname)s - %(message)s